# For this to work you must setup a local env var for MONGODB_URI that points to the
# production database server.
docker run -it -p 3122:3122 -e 'APP_URL=http://localhost:3122' -e 'BASE_URL=http://localhost:3122' -e 'NODE_ENV=production' 585172581592.dkr.ecr.ap-southeast-2.amazonaws.com/vly-alpha:master

